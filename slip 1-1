public class PrimeNumbersInArray {

    public static void main(String[] inputs) {
        if (inputs.length == 0) {
            System.out.println("Usage: java PrimeNumbersInArray <num1> <num2> <num3> ...");
            System.out.println("Example: java PrimeNumbersInArray 2 3 4 5 10 13 17 20");
            return;
        }

        System.out.println("Prime numbers from the provided list:");
        int totalPrimes = 0;

        for (String input : inputs) {
            try {
                int value = Integer.parseInt(input);
                if (isPrime(value)) {
                    System.out.print(value + " ");
                    totalPrimes++;
                }
            } catch (NumberFormatException ex) {
                System.out.println("\n(Skipping non-integer input: '" + input + "')");
            }
        }

        if (totalPrimes == 0) {
            System.out.println("No prime numbers were found in the input.");
        }

        System.out.println();
    }

    public static boolean isPrime(int number) {
        if (number <= 1) {
            return false;
        }
        int limit = (int) Math.sqrt(number);
        for (int i = 2; i <= limit; i++) {
            if (number % i == 0) {
                return false;
            }
        }
        return true;
    }
}
